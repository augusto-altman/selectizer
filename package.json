  "name": "selectizer",
  "description": "Selectizer is a build tool made for Node.js that compile javascript projects. It allows to divide an entire project into small javascript files (modules) and then select what pieces of code to include in a specific build.",
  "version": "0.1.18",
  "repository": {
    "url": "https://github.com/augusto-altman/selectizer.git"
  },
  "contributors": [
    {
      "name": "Matias Carranza",
      "email": "matiascarranza@gmail.com"
    }
  ],
  "main": "./dist/lib/selectizer.min",
  "keywords": [
    "modularization",
    "modules",
    "compile",
    "build",
    "dependencies",
    "select",
    "file selection",
    "modules selection",
    "build customization",
    "cutomization",
    "build tool"
  ],
  "bin": {
    "selectizer": "./dist/bin/selectizer"
  },
  "dependencies": {
    "mkdirp": "~0.3.4",
    "requirejs": "~2.1.4"
  },
  "engines": {
    "node": "*"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-concat": "^0.4.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-nodeunit": "^0.4.1",
    "grunt-contrib-uglify": "^0.5.0"
  },
  "license": "MIT"
}
